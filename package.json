{
  "name": "@appjumpstart/booster",
  "version": "1.3.0",
  "description": "A Knex.js utility to make setting up a database super easy",
  "main": "index.js",
  "bin": {
    "booster": "index.js"
  },
  "scripts": {
    "test": "jest",
    "test:complete": "docker-compose up -d && jest && docker-compose down",
    "lint": "eslint --ext js index.js tests"
  },
  "keywords": [
    "knex",
    "database",
    "setup",
    "postgres",
    "pg"
  ],
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.1",
    "knex": "^0.15.2",
    "meow": "^5.0.0",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "execa": "^1.0.0",
    "jest": "^23.1.0",
    "np": "^3.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appjumpstart/booster.git"
  },
  "bugs": {
    "url": "https://github.com/appjumpstart/booster/issues"
  },
  "homepage": "https://github.com/appjumpstart/booster#readme",
  "eslintConfig": {
    "root": true,
    "env": {
      "jest/globals": true
    },
    "extends": [
      "standard"
    ],
    "plugins": [
      "jest"
    ]
  }
}
